class כתר:
    def __init__(self):
        self.כתר״ = "כתר״"
        self.𓎡𓏏𓂋 = "𓎡𓏏𓂋"
        self.Keter = "Keter"
        self.Crown = "Crown"
        self.RowText = "RowText"

class חכמה:
    def __init__(self):
        self.𓎡𓉔𓎡𓅓𓉔 = "𓎡𓉔𓎡𓅓𓉔"
        self.Chochmah = "Chochmah"
        self.Wisdom = "Wisdom"
        self.RowText = "RowText"

